    WSGIPath: h4h/wsgi.py
    StaticFiles: /static/=static/
  aws:elasticbeanstalk:application:environment:
    DJANGO_SETTINGS_MODULE: h4h.settings
container_commands:
  01_make_migrations:
    command: "python manage.py makemigrations; python manage.py migrate"
    leader_only: true